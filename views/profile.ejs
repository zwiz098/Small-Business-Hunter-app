<!doctype html>
<html>
<head>
    <title>SBS</title>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <link rel="stylesheet" href="style4.css">
</head>
<body>
<div class="container">

    <div class="page-header text-center">
        <h1><span class="fa fa-anchor"></span><%= user.local.username %></h1>
        <h2><span></span><%= user.local.email%></h2>
        <h2><span></span><%= userInfo.compName%></h2>
        <a href="/logout" class="btn btn-default btn-sm">Logout</a>
          <p><a href="/feed">Search businesses</a></p>
    </div>

    <div class="row">
        <div class="col-sm-6">
          <form class="messages" action="/personal" method="post">
            <input type="button" name="" value="">
          </form>
          <h3>Profile</h3>
          <ul class="messages">
          <% for(var i=0; i<messages.length; i++) {%>
            <li class="message">
              <img src="<%= messages[i].house[0] %>" alt="<%= user._id%>">
              <img src="<%= messages[i].house[1] %>" alt="<%= user._id%>">
              <span><%= messages[i].typ %></span>
              <span><%= messages[i].bs %></span>
              <span><%= messages[i].tags %></span>
              <span><i class="fa fa-trash" aria-hidden="true"></i></span>
            </li>
          <% } %>
          </ul>
                <p><a href="/personal">Edit Account</a></p>

                  <!-- for loop for bookmarks with li and ancor tag. -->
                  <!-- <a href="/post/<%= messages[i]._id %>"><img src="../<%= messages[i].house[1] %>" alt="image"></a> -->
        </div>

      </div>
</div>

<script src="main.js"></script>

</body>
</html>
